
# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /script/
RewriteRule ^index\.php$ - [L]
RewriteRule ^admin/(.*) /script/wp-admin/$1?rswy_hide_my_wp=1234 [QSA,L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /script/index.php [L]
</IfModule>

# END WordPress
